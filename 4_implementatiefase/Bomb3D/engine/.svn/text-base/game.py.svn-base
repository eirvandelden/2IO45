# -*- coding: utf-8 -*-

import gamestate

class tournament():
  ### available properties:
  # UUID: identifier for the game
  # gamephase: the phase that the game is in.
  # time_start: the time that the game starts with
  # map: The map on wich the players will be playing
  
  def __init__(self, UUID, gamephase, time_start, map):
    self.UUID = UUID
    self.gamephase = gamephase
    self.time_start = time_start
    self.map = map